#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n,d,s=0,t,r=0;
    scanf("%d",&n);
    t=n;
    while(n>0)
    {
        d=n%10;
        n=n/10;
        s=(s+d*d*d);
           r=r+(d*d*d*d);
    }
    if(s==t)
    {
        printf("Yes");
    }
    else if(r==t){
     printf("Yes");
    }
   else
   {
       printf("No");
   }
    return 0;
}
