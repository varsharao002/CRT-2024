#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int x1,x2,y1,y2; 
    scanf("%d %d %d %d",&x1,&y1,&x2,&y2); 
    float m1=((float)(x1+x2)/2);
    float m2=((float)(y1+y2)/2); 
    printf("Binoy's house is located at (%.1f,%.1f)",m1,m2);
    return 0;
}
