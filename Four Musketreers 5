#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main(){
    
    int x1,x2,x3,y1,y2,y3;
    scanf("%d",&x1);
    scanf("%d",&y1);
    scanf("%d",&x2);
    scanf("%d",&y2);
    scanf("%d",&x3);
    scanf("%d",&y3);
    printf("%.1f\n%.1f",(float)(x1+x2+x3)/3,(float)(y1+y2+y3)/3);

    return 0;
}
